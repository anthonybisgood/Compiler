func fib(x:int):int{
    if x <= 1{return x}
    return fib(x-1) + fib(x-2)
}
func main(){   
    var n : int 
    n = 20
    print fib(n)
    n = 8 
    print fib(n)
    n = 12 
    print fib(n)
