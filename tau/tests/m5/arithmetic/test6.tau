func add(x:int, y:int):int{
    return x+y
}
func subtract(x:int, y:int):int{
    return x-y
}
func multiply(x:int, y:int):int{
    return x*y
}
func divide(x:int, y: int):int{
    return x/y
}
func main():void{
    var x : int 
    var y: int 
    x = 1000
    y = 20
    print add(x,y)
    print subtract(x,y)
    print multiply(x,y)
    print divide(x,y)
}