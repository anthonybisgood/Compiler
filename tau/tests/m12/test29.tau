func add(x:int, y:int):int{
    if x > 0 and y > 0{
        return x+y
    }
    else{
        return 0
    }
}
func subtract(x:int, y:int):int{
    if x > 0 and y > 0{
        return x - y
    }
    else{
        return 0
    }
}
func multiply(x:int, y:int):int{
    if x > 0 and y > 0{
        return x * y
    }
    else{
        return 0
    }
}
