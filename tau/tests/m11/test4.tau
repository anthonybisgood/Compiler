func isEven():bool{
    var isEven:int
    var i : int
    var x : int 
    x = 97
    i = 1
    isEven = 1
    while i <= x{
        isEven = -isEven
        i = i + 1
    }
    if isEven == 1{
        return true
    }
    else{
        return false
    } 
}

func main() : void{
    var x : int 
    x = 97
    print isEven()
}